,\r\n    category: \"\",\r\n    price: \"\",\r\n  });\r\n\r\n  const changeHandler = (e) => {\r\n    setForm({...form, [e.target.name]: e.target.value});\r\n  };\r\n  const message = useMessage();\r\n\r\n  useEffect(() => {\r\n    message(error);\r\n    clearError();\r\n  }, [error, message, clearError]);\r\n\r\n  const createAdvertHandler = async (e) => {\r\n    e.preventDefault();\r\n\r\n    const userId = await selectId(auth.userId);\r\n\r\n    try {\r\n      const result = await request(`/api/auth/user/${auth.userId}`, \"GET\", null, {Authorization: `Bearer ${auth.token}`});\r\n      const [user] = result;\r\n      const data = await request(\"/api/advert/create\", \"POST\", {\r\n        ...form,\r\n        userName: user.userName,\r\n        created: new Date()\r\n      }, {Authorization: `Bearer ${auth.token}`});\r\n      console.log(\"DATA\", data);\r\n      history.push(`/advert/${data.advert.insertId}`);\r\n      message(data.message);\r\n    } catch (e) {\r\n    }\r\n  };\r\n  return (\r\n    <section className='create-advert-section'>\r\n      <h2>Create advert</h2>\r\n      <AdvertForm\r\n        changeHandler={changeHandler}\r\n        submitMethod={createAdvertHandler}\r\n        defaultValues={[]}\r\n      />\r\n    </section>\r\n  );\r\n};","C:\\Users\\Roman\\Desktop\\SecureAppDiplom\\client\\src\\components\\UserAdvertsPage.js",[],"C:\\Users\\Roman\\Desktop\\SecureAppDiplom\\client\\src\\components\\Dashboard.js",[],"C:\\Users\\Roman\\Desktop\\SecureAppDiplom\\client\\src\\components\\UserPage.js",["258"],"import React, {useState, useCallback, useContext, useEffect} from \"react\";\r\nimport {useParams, useHistory, Link} from \"react-router-dom\";\r\nimport {useHttp} from \"../hooks/httpHook\";\r\nimport {AuthContext} from \"../context/AuthContext\";\r\nimport {LoadingPage} from \"./LoadingPage\";\r\n\r\nexport const UserPage = () => {\r\n  const {token} = useContext(AuthContext);\r\n  const auth = useContext(AuthContext);\r\n  const {request, loading} = useHttp();\r\n  const [user, setUser] = useState(null);\r\n  const history = useHistory();\r\n\r\n  const userId = useParams().id;\r\n  const id = auth.userId;\r\n\r\n  console.log(\"idFromURL\", userId);\r\n  const getUser = useCallback(async () => {\r\n    try {\r\n      const result = await request(`/api/auth/user/${id}`, \"GET\", null, {Authorization: `Bearer ${token}`});\r\n      console.log(result);\r\n      setUser(...result);\r\n    } catch (e) {\r\n    }\r\n  }, [token, id, request]);\r\n\r\n  useEffect(() => {\r\n    getUser();\r\n  }, [getUser]);\r\n\r\n  const editRedirect = () => {\r\n    history.push(`/editUser/${id}`);\r\n  };\r\n\r\n  const removeRedirect = () => {\r\n    history.push(`/deleteUser/${id}`);\r\n  };\r\n\r\n  if (loading) {\r\n    return <LoadingPage/>;\r\n  }\r\n  return (\r\n    <>\r\n      {user &&\r\n      <div className='user-page'>\r\n        <p>User name: {user.userName}</p>\r\n        <p>First name: {user.firstName}</p>\r\n        <p>Last name: {user.lastName}</p>\r\n        <p>Email: {user.email}</p>\r\n        <p>Phone number: {user.phone}</p>\r\n        <div className='edit-remove-btns'>\r\n          <button className='btn' onClick={editRedirect}>Edit</button>\r\n          <button className='btn' onClick={removeRedirect}>Delete</button>\r\n        </div>\r\n      </div>\r\n      }\r\n    </>\r\n  );\r\n};","C:\\Users\\Roman\\Desktop\\SecureAppDiplom\\client\\src\\components\\AllAdvertsPage.js",[],"C:\\Users\\Roman\\Desktop\\SecureAppDiplom\\client\\src\\components\\AllUsersPage.js",[],"C:\\Users\\Roman\\Desktop\\SecureAppDiplom\\client\\src\\components\\AdvertPage.js",["259"],"import React, {useState, useCallback, useContext, useEffect} from \"react\";\r\nimport {useParams, useHistory} from \"react-router-dom\";\r\nimport {useHttp} from \"../hooks/httpHook\";\r\nimport {AuthContext} from \"../context/AuthContext\";\r\nimport {LoadingPage} from \"./LoadingPage\";\r\nimport {AdvertCard} from \"./statelessComponents/AdvertCard\";\r\nimport {selectId} from \"../hooks/selectId\";\r\n\r\nexport const AdvertPage = () => {\r\n  const {request, loading} = useHttp();\r\n  const [advert, setAdvert] = useState(null);\r\n  const auth = useContext(AuthContext);\r\n  const history = useHistory();\r\n  const advertId = useParams().id;\r\n\r\n  const userId = auth.userId;\r\n\r\n  const getAdvert = useCallback(async () => {\r\n    try {\r\n      const [result] = await request(`/api/advert/byId/${advertId}`, \"GET\", null,\r\n        {Authorization: `Bearer ${auth.token}`});\r\n      console.log(\"RESULT\", result);\r\n      if (userId !== result.ownerID) {\r\n        await request(`/api/advert/editAdvert/${result.id}`, \"PATCH\", {\r\n          views: result.views + 1\r\n        }, {Authorization: `Bearer ${auth.token}`});\r\n      }\r\n      setAdvert(result);\r\n    } catch (e) {\r\n    }\r\n  }, [auth.token, advertId, request, userId]);\r\n\r\n  useEffect(() => {\r\n    getAdvert();\r\n  }, [getAdvert]);\r\n\r\n  const editRedirect = () => {\r\n    history.push(`/editAdvert/${advert.id}`);\r\n  };\r\n\r\n  const removeRedirect = () => {\r\n    history.push(`/deleteAdvert/${advert.id}`);\r\n  };\r\n\r\n  if (loading) {\r\n    return <LoadingPage/>;\r\n  }\r\n\r\n  return (\r\n    <>\r\n      {!loading && advert &&\r\n      <section className='advert-section'>\r\n        <AdvertCard advert={advert}/>\r\n        {userId === advert.ownerID &&\r\n        <div className='edit-remove-btns'>\r\n          <button className='btn' onClick={editRedirect}>Edit</button>\r\n          <button className='btn' onClick={removeRedirect}>Delete</button>\r\n        </div>\r\n        }\r\n      </section>\r\n      }\r\n    </>\r\n  );\r\n};","C:\\Users\\Roman\\Desktop\\SecureAppDiplom\\client\\src\\components\\EditAdvertPage.js",[],"C:\\Users\\Roman\\Desktop\\SecureAppDiplom\\client\\src\\components\\RemoveAdvertPage.js",[],"C:\\Users\\Roman\\Desktop\\SecureAppDiplom\\client\\src\\components\\EditUserPage.js",["260"],"import React, {useState, useEffect, useContext, useCallback} from \"react\";\r\nimport {useHttp} from \"../hooks/httpHook\";\r\nimport {AuthContext} from \"../context/AuthContext\";\r\nimport {useParams, useHistory} from \"react-router-dom\";\r\nimport {LoadingPage} from \"./LoadingPage\";\r\nimport {useMessage} from \"../hooks/errorHook\";\r\nimport {UserForm} from \"./statelessComponents/UserForm\";\r\n\r\nexport const EditUserPage = () => {\r\n  const auth = useContext(AuthContext);\r\n  const [form, setForm] = useState({\r\n    firstName: \"\",\r\n    lastName: \"\",\r\n    userName: \"\",\r\n    email: \"\",\r\n    phone: \"\",\r\n  });\r\n  const [user, setUser] = useState(null);\r\n  const {request, loading} = useHttp();\r\n  const userId = useParams().id;\r\n  const history = useHistory();\r\n  const message = useMessage();\r\n\r\n  // console.log(\"tyt\", auth.userId);\r\n\r\n  const getUser = useCallback(async () => {\r\n    try {\r\n      const result = await request(`/api/auth/user/${auth.userId}`, \"GET\", null,\r\n        {Authorization: `Bearer ${auth.token}`});\r\n      setUser(...result);\r\n    } catch (e) {\r\n    }\r\n  }, [auth.token, auth.userId, request]);\r\n\r\n  useEffect(() => {\r\n    getUser();\r\n  }, [getUser]);\r\n\r\n  function validateEmail(email) {\r\n    const re = /^(([^<>()[\\]\\\\.,;:\\s@\"]+(\\.[^<>()[\\]\\\\.,;:\\s@\"]+)*)|(\".+\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))$/;\r\n    return re.test(String(email).toLowerCase());\r\n  }\r\n\r\n  const changeHandler = (e) => {\r\n    setForm({...form, [e.target.name]: e.target.value});\r\n  };\r\n\r\n  const editUserHandler = async (e) => {\r\n    e.preventDefault();\r\n\r\n    try {\r\n      if (form.email && !validateEmail(form.email)) {\r\n        message(\"Invalid email\");\r\n        return;\r\n      }\r\n      console.log(\"перед запросом\");\r\n      await request(`/api/auth/editUser/${auth.userId}`, \"PATCH\", {\r\n          firstName: form.firstName ? form.firstName : user.firstName,\r\n          lastName: form.lastName ? form.lastName : user.lastName,\r\n          userName: form.userName ? form.userName : user.userName,\r\n          email: form.email ? form.email : user.email,\r\n          phone: form.phone ? form.phone : user.phone,\r\n        },\r\n        {Authorization: `Bearer ${auth.token}`});\r\n      console.log(\"I am Here!\");\r\n      history.push(`/userPage/${auth.userId}`);\r\n    } catch (e) {\r\n    }\r\n  };\r\n\r\n  if (loading) {\r\n    return <LoadingPage/>;\r\n  }\r\n  return (\r\n    <div>\r\n      {user &&\r\n      <div>\r\n        <h2>Edit user</h2>\r\n        <UserForm\r\n          changeHandler={changeHandler}\r\n          submitMethod={editUserHandler}\r\n          defaultValues={[user.firstName, user.lastName, user.userName, user.email, user.phone]}\r\n          isCreatePage={false}\r\n        />\r\n      </div>}\r\n    </div>\r\n  );\r\n};","C:\\Users\\Roman\\Desktop\\SecureAppDiplom\\client\\src\\components\\RemoveUserPage.js",["261"],"import React, {useCallback, useContext} from \"react\";\r\nimport {useParams} from \"react-router-dom\";\r\nimport {useHttp} from \"../hooks/httpHook\";\r\nimport {AuthContext} from \"../context/AuthContext\";\r\nimport {useHistory} from \"react-router-dom\";\r\n\r\nexport const RemoveUserPage = () => {\r\n  const auth = useContext(AuthContext);\r\n  const {request} = useHttp();\r\n  const userId = useParams().id;\r\n  //TODO  рассказать про то как можно удалить юзера используя используя useParams().id\r\n  const history = useHistory();\r\n\r\n  const removeUser = useCallback(async (e) => {\r\n    e.preventDefault();\r\n\r\n    try {\r\n      await request(`/api/auth/deleteUser/${auth.userId}`, \"DELETE\", null, {Authorization: `Bearer ${auth.token}`});\r\n      auth.logout();\r\n      history.push(\"/\");\r\n    } catch (e) {\r\n    }\r\n  }, [auth, history, request]);\r\n\r\n  return (\r\n    <div className='dele